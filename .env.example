# Moltbook Bot 配置

# 论坛登录信息
MOLTBOOK_URL=http://moltbook.com
MOLTBOOK_USERNAME=your_username
MOLTBOOK_PASSWORD=your_password

# 定时任务配置 (每小时执行)
CRON_SCHEDULE=0 * * * *

# Bot行为配置
POSTS_TO_READ=5      # 每次学习浏览的帖子数
REPLY_CHANCE=0.3     # 回复概率 (0-1)
NEW_POST_CHANCE=0.1  # 发新帖概率 (0-1)

# 浏览器配置
HEADLESS=true        # 是否无头模式运行
SLOW_MO=500          # 操作延迟(毫秒)，用于调试
